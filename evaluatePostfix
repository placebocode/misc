#include<stdio.h>
#include<string.h>

int isDigit(char ch){
	if(ch >= '0' && ch <= '9') return 1;
	return 0;
}

int stack[20];
int top = -1;

void push(int x)
{
    stack[++top] = x;
}

int pop()
{
    return stack[top--];
}

int main()
{
    char exp[20];
    char *e;
    int n1,n2,n3,num;
    printf("Enter the expression :: ");
    scanf("%s",exp);
    for(int i=strlen(exp)-1;i>=0;i--)
    {
        char *e = &exp[i];
        if(isDigit(*e))
        {
            num = *e - '0';
            push(num);
        }
        else
        {
            n1 = pop();
            n2 = pop();
            switch(*e)
            {
            case '+':
            {
                n3 = n1 + n2;
                break;
            }
            case '-':
            {
                n3 = n1 - n2;
                break;
            }
            case '*':
            {
                n3 = n1 * n2;
                break;
            }
            case '/':
            {
                n3 = n2 / n1;
                break;
            }
            }
            push(n3);
        }
    }
    printf("\nThe result of expression %s  =  %d\n\n",exp,pop());
    return 0;
}
